#!/usr/bin/env bash
# Script to count occurrences of IP and HTTP code in Apache logs and display sorted results
awk '{count[$1, $9]++} 
     END {for (key in count) {
           split(key, arr, SUBSEP)
           printf "%5d %s %s\n", count[key], arr[1], arr[2]
     }}' apache-access.log | sort -nr
